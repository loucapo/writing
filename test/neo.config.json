{

  "baseUrl": "http://localhost:10080",
  "dbapi": "http://localhost:4667",
  "featuresDir": "tests-neo/features",
  "stepsDir": "tests-neo/steps",
  "reportDir": "reports",
  "reporters": [{"name": "marvin-reporter"}],
  "threads": 1,

