{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "main": "app/index.js",
  "scripts": {
    "start": "node app/index.js",
    "start_dev": "nodemon --ignore app/src/swagger --harmony-async-await app/index.js",
    "test": "mocha --opts ./app/tests/mocha.opts --compilers js:babel-register ./app/tests/*/*.js -w",
    "testOnce": "NODE_ENV=local LOGGING_TRANSPORTS=console mocha --opts ./app/tests/mocha.opts --compilers js:babel-register ./app/tests/*/*.js",
    "coverage": "nyc npm run testOnce",
    "lint": "eslint .",
    "validate": "npm ls",
    "licenses": "license-checker --production --csv | tee licenses.csv"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.6"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "config": "^1.21.0",
    "dagon": "reharik/dagon",
    "enumify": "^1.0.4",
    "json-schema-deref-sync": "^0.3.2",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-compress": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-cors": "0.0.16",
    "koa-errorhandler": "^0.1.1",
    "koa-generic-session": "^1.11.3",
    "koa-jwt": "^1.3.1",
    "koa-logger": "^2.0.0",
    "koa-papers": "reharik/koa-papers",
    "koa-response-time": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-send": "^3.2.0",
    "os": "^0.1.1",
    "pg-async": "^2.4.0",
    "pg-format": "^1.0.4",
    "puresql": "^1.2.0",
    "swagger-jsdoc": "^1.7.0",
    "winston": "^2.3.0",
    "winston-logstash": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-3": "^6.11.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "eslint": "^3.4.0",
    "istanbul": "^1.0.0-alpha.2",
    "license-checker": "^5.1.2",
    "mocha": "^3.2.0",
    "nodemon": "^1.10.2",
    "nyc": "^10.1.2",
    "precommit-hook-eslint": "^3.0.0",
    "supertest": "^2.0.0",
    "testdouble": "^1.10.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ]
  },
  "pre-commit": [
    "testOnce"
  ]
}
