{
  "app": {
    "port": "3000",
    "env": "NODE_ENV",
    "keys": "APPLICATION_KEYS",
    "spa_url": "SPA_URL",
    "jwt_secret": "JWT_SECRET",
    "wk_serve_url": "WK_SERVE_URL",
    "swagger_ui_url": "SWAGGER_UI_URL",
    "logging_transports": "LOGGING_TRANSPORTS",
    "logging_level": "LOGGING_LEVEL"
  },
  "postgres": {
    "config" :{
      "user": "POSTGRES_USER",
      "database": "POSTGRES_DATABASE",
      "host"     : "POSTGRES_HOST",
      "password": "POSTGRES_PASSWORD",
      "port": "POSTGRES_PORT",
      "max": "10",
      "idleTimeoutMillis": "30000"
    }
  },
  "frontEnd": {
    "origin": "FRONT_END_ORIGIN"
  }
}
