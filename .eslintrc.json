{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "react"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "semi": 2,
    "display-name": 0
  },
  "env": {
    "browser": true,
    "node" :true
  },
  "globals": {
    "Symbol": true
  }
}