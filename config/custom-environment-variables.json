{
  "app": {
    "port": "4777",
    "env": "NODE_ENV",
    "keys": "APPLICATION_KEYS",
    "spa_url": "SPA_URL",
    "wk_api_url": "WK_API_URL",
    "consumer_key": "CONSUMER_KEY",
    "consumer_secret": "CONSUMER_SECRET"
  },
  "postgres": {
    "config" :{
      "user": "POSTGRES_USER",
      "database": "POSTGRES_DATABASE",
      "host"     : "POSTGRES_HOST",
      "password": "POSTGRES_PASSWORD",
      "port": "POSTGRES_PORT",
      "max": "10",
      "idleTimeoutMillis": "30000"
    }
  }
}
