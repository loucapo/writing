<% users = [5, 20] %>
---
:project: 'ECO-1218'
:suite:
  # if you don't have a name here, it will just use timestamp as the suite
  :name: baseline-POB-course
  :tests:<% for user in users %>
    :test<%= user %>:
      :template: 'basic'
      :domain: "hm-staging.mnv-tech.com"
      :users: <%= user %>
      :name: basic-with-<%= user %>-users
      :duration: 30
      :postsleep: 10
      :enrollments: <%= Paths.join('ECO-1218/manual_enrollments.csv', :artifacts) %>
      :course_view_count: 3
      :render: 'graphs1'<% end %>
