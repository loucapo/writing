{
  "name": "wk_frontend",
  "version": "0.0.0",
  "private": true,
  "keywords": [
    "react",
    "react-router"
  ],
  "main": "index.js",
  "scripts": {
    "build": "webpack -p --config ./app/webpack.config.js --progress --profile --colors",
    "start": "webpack-dev-server --progress --config ./app/webpack.config.js",
    "startQA": "webpack-dev-server --progress --config ./app/webpack.config.js",
    "test": "BABEL_DISABLE_CACHE=1 NODE_ENV=test mocha --opts ./tests/mocha.opts --compilers js:babel-core/register --require babel-polyfill ./app/tests/*/*.tests.js",
    "testOnce": "BABEL_DISABLE_CACHE=1 NODE_ENV=test mocha --compilers js:babel-core/register --require babel-polyfill --opts ./tests/mocha.opts ./app/tests/*/*.tests.js",
    "coverage": "nyc npm run testOnce",
    "lint": "eslint .",
    "validate": "npm ls",
    "licenses": "license-checker --production --csv | tee licenses.csv"
  },
  "license": "UNLICENSED",
  "engines": {
    "node": ">=4.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.22.0",
    "classnames": "^2.2.5",
    "draft-js": "^0.10.0",
    "file-loader": "^0.9.0",
    "fixed-data-table": "^0.6.3",
    "freakin-react-forms": "reharik/freakin-react-forms",
    "jwt-decode": "^2.1.0",
    "ml-react-cdl-icons": "1.3.2",
    "ml-react-rte": "^0.1.1",
    "moment": "^2.17.1",
    "precommit-hook-eslint": "^3.0.0",
    "prop-types": "15.5.8",
    "react": "15.4.2",
    "react-cookie": "^1.0.5",
    "react-dom": "15.4.2",
    "react-draft-wysiwyg": "^1.9.5",
    "react-hot-loader": "^3.0.0-beta.3",
    "react-nl2br": "^0.2.0",
    "react-redux": "^5.0.4",
    "react-router": "3.0.4",
    "react-router-redux": "^4.0.8",
    "redux": "^3.3.1",
    "redux-api-middleware": "^1.0.2",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.14.3",
    "redux-thunk": "^2.0.1",
    "resolve-url-loader": "^1.6.1",
    "url-loader": "^0.5.7"
